<Window x:Class="SalonNamestaja.UI.DodajIzmeniTipNamestaja"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SalonNamestaja.UI"
        mc:Ignorable="d"
        Title="Dodavanje tipa namestaja" Height="221.77" Width="300">
    <Grid>
        <Button Content="Sacuvaj" HorizontalAlignment="Left" Margin="51,153,0,0" VerticalAlignment="Top" Width="75" Click="btnSacuvaj_Click"/>
        <Button Content="Otkazi" HorizontalAlignment="Left" Margin="152,153,0,0" VerticalAlignment="Top" Width="75" Click="btnOtkazi_Click"/>
        <Label Content="Ime tipa namestaja" HorizontalAlignment="Left" Margin="10,73,0,0" VerticalAlignment="Top" Width="126"/>
        <TextBox x:Name="tbNazivTipaNamestaja" HorizontalAlignment="Left" Height="23" Margin="127,77,0,0" TextWrapping="Wrap" Text="{Binding Path=Naziv}" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
